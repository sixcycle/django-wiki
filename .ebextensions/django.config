option_settings:
  aws:elasticbeanstalk:container:python:
    WSGIPath: SixcycleWiki/SixcycleWiki/wsgi.py

files:
  "/etc/httpd/conf.d/cors.conf" :
    mode: "000644"
    owner: root
    group: root
    content: |
      <Location "/">
        Header set Access-Control-Allow-Origin: *
        SetOutputFilter DEFLATE
      </Location>