commands:
  01_wsgipass:
    command: 'echo "WSGIPassAuthorization On" >> ../wsgi.conf'
  02_symbolick_link:
    command: 'ln -s /opt/python/current/app/SixcycleWiki/lib/wiki/src/wiki/'
  03_migrate:
    command: 'source /opt/python/run/venv/bin/activate & /opt/python/current/app/SixcycleWiki/manage.py migrate --no-input'
  03_collect_static:
    command: 'source /opt/python/run/venv/bin/activate & /opt/python/current/app/SixcycleWiki/manage.py collectstatic --no-input'